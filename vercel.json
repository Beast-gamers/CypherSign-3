{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "API_KEY": "@api-key" 
  },
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "regions": [
    "sfo1", "iad1"
  ],
  "cleanUrls": true
}
